.page{
  width: 100vw;
  height: 100vh;
}
.content{
  padding:0 30rpx;
  margin-bottom: 30rpx;
}
.tip{
  font-weight: 600;
  height: 80rpx;
  line-height: 80rpx;
  padding: 0 30rpx;
  color:#1296db;
}
.inp{
  border: 1px #ccc solid;
  padding: 20rpx 30rpx;
  font-size: 30rpx;
  border-radius: 10rpx;
}
.con{
  height: auto;
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
  padding: 0 20rpx;
}
.box{
  width: 48%;
  border: 1px #f5f5f5 solid;
  margin:10rpx 0;
  background: #fff;
  display: flex;
  flex-direction: column;
}
.imgbox{
  height: 200rpx;
  border-radius: 20rpx;
}
.img{
  width: 100%;
  height: 100%;
}
.pri{
  height: 50rpx;
  line-height: 50rpx;
  font-size: 28rpx;
  color: red;
  text-indent: 20rpx;
}
.name{
  color: #1296db;
}
.empty{
  width: 100%;
  height:600rpx;
}
.emptyimg{
  width: 100%;
  height: 100%;
}