.title{
  font-weight: 600;
  padding: 30rpx;
  /* background: #fff; */
}
.content{
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  border-bottom: 1px #ccc solid;
  position: relative;
  background: #fff;
}
.contentlast{
  padding: 20rpx 30rpx;
  display: flex;
  align-items: center;
  border: none;
  position: relative;
  background: #fff;
  margin-bottom: 100rpx;
}
.icon{
  line-height:26px;
}
.tip{
  margin-left:20rpx;
  font-size: 28rpx;
  color: #333;
}
.del{
  position: absolute;
  right: 30rpx;
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  z-index: 100;
}