    <view class="tip">个人秘钥</view>
    <view class="inpbox">
      <input placeholder="系统将自动生成您的秘钥" type="number" disabled class="inp disabled"></input>
    </view>
  </view> -->
  <button class="submit" bindtap="submit">提 交</button>
</view>